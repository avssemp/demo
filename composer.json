{
    "name": "avssemp/demo",
    "description": "demo",
    "license": "MIT",
    "authors": [
        {
            "name": "guptaamitmca11",
            "email": "guptaamitmca11@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Avssemp\\Demo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avssemp\\Demo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit",
        "phpunit": "vendor/bin/phpunit",
        "coverage-test": "vendor\\bin\\phpunit --coverage-html /test-reports/coverage",
        "format": "vendor\\bin\\php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
